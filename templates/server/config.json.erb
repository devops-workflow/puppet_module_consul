{
	"bootstrap": <%= @bootstrap %>,
	"server": true,
	"datacenter": "core",
	"data_dir": "/var/consul",
	"encrypt": "<%= @encrypt_string %>",
	"log_level": "INFO",
	"enable_syslog": true,
	"disable_update_check": true,
	"rejoin_after_leave": true,
	<% if @bootstrap -%>
		"enable_syslog": true
	<% else -%>
		"start_join": [<% @start_join.each do |j| -%><% if j == @start_join.last -%>"<%= j %>"<% else -%>"<%= j %>", <% end -%><% end -%>]
	<% end -%>
}
